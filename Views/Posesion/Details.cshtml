@model Biblioteca.Models.dbModels.Prestamo

@{
    ViewData["Title"] = "Detalles de Prestamo";
}

@section Styles {
    <link rel="stylesheet" href="~/css/administrar-elementos.css" />
}

<h1>Detalles de Prestamo</h1>

<div>
    <h4>Prestamo</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.InicioPrestamo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.InicioPrestamo)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FinPrestamo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FinPrestamo)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Libro)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Libro.Titulo)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Libro.Imagen)
        </dt>
        <dd class="col-sm-10">
            <img class="foto" src="@Html.DisplayFor(model => model.Libro.Imagen)" alt="" />
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index" class="btn btn-dark">Volver</a>
</div>
